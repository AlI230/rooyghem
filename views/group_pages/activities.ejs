<%- include("../particals/group_nav.ejs") -%>
<% moment.locale("nl-be"); %>
<h1>Activiteiten Pagina</h1>

<ul>
    <% activities.forEach(function(activity){ %>
        <li><a href="/activities/activity/<%= activity.activity_id %>"><%= activity.title %>, <%= moment(activity.start_date).format('LLL') %> - <%= moment(activity.end_date).format("LT") %></a></li>
    <% }) %>
</ul>

