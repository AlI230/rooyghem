<main class="container" id="backofficecontainer">
    <%- include('./particals/back_office.ejs') %>
    <div class="userpanel">
        <h2>Gebruikers</h2>
        <div class="users">
            <% users.forEach(function(user) { %>
                <div class="userinfo">
                    <p><%= user.username %>
                        <div class="buttons">
                            <a href="/users/<%= user.user_id %>">bewerk</a>
                            <a href="/users/delete/single/<%= user.user_id %>">delete</a>
                        </div>
                    </p>
                    
                </div>
            <% }) %>
        </div>
        <a class="addLink" href="/users/create">Nieuwe gebruiker</a>
    </div>
</main>